// DB: analytics

Table warehouses {
  warehouse_id int [pk, not null]
  warehouse_name varchar [not null]
  country varchar
  created_at timestamp [default: 'now()']
  Note: 'ordering: warehouse_id'
}
Table products {
  sku varchar [pk, not null]
  product_name varchar [not null]
  category varchar
  unit_cost decimal(10,2) [not null]
  active boolean
  attrs_json json
  created_at timestamp [default: 'now()']
  Note: 'ordering: sku\nqe.ranges: unit_cost[0..None]'
}
Table inventory {
  sku varchar [not null]
  warehouse_id int [not null]
  restock_ts timestamp [not null]
  stock_qty int [not null]
  indexes {
    (sku, warehouse_id) [pk]
  }
  Note: 'ordering: restock_ts, sku, warehouse_id\npartitioning: to_date(restock_ts)\nqe.ranges: stock_qty[0..None]'
}

// Foreign Keys
Ref: inventory.sku > products.sku
Ref: inventory.warehouse_id > warehouses.warehouse_id
